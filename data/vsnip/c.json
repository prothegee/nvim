{
	"comment spacer": {
		"prefix": "_comment_spacer",
		"body": [
			"// --------------------------------------------------------- //"
		]
	},
    "struct": {
        "prefix": "_struct",
        "body": [
            "struct $1 {",
            "    // data",
            "}; // $1"
        ]
    },
    "struct typedef 1": {
        "prefix": "_struct_typedef_1",
        "body": [
            "typedef struct {",
            "    // data",
            "} $1;"
        ]
    },
	"struct typedef 2": {
        "prefix": "_struct_typedef_2",
        "body": [
            "typedef struct $1 {",
            "    // data",
            "} $2;"
        ]
    },
	"if not define define": {
		"prefix": "_ifndef_def_endif",
		"body": [
			"#ifndef $1",
			"#define $1",
			"#endif // $1"
		]
	},
	"int main": {
		"prefix": "_int_main",
		"body": [
			"int main() {",
            "    return 0;",
            "}"
		]
	},
	"int main argc argv": {
		"prefix": "_int_main_argc_argv",
		"body": [
			"int main(int argc, char *argv[]) {",
            "    return 0;",
            "}"
		]
	},
	"head file info": {
		"prefix": "_head_file_info",
		"body": [
			"/// ### file: $TM_FILENAME_BASE",
            "/// ",
            "/// ### author: $1",
            "/// ",
            "/// ---------------------------------------------------------",
            "/// ",
            "/// ### brief:",
            "/// n/a",
            "/// ",
            "/// ### note:",
            "/// n/a",
            "/// ",
            "/// ---------------------------------------------------------",
            "/// ",
            "/// ### copyright:",
            "/// ",
            "/// - (c) $2 $3 // $4 // $5",
            "/// ",
            "/// ### contributors:",
            "/// ",
            "/// - $3 // $4 // $5"
		]
	}
}

